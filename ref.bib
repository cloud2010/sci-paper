	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/13547500500216660},
	keywords = {atherosclerosis, farnesol, geranylgeraniol, mevalonate pathway, Prenylated protein, statin},
	pages = {219--237},
	file = {Ferri et al_2005_Lipid-modified proteins as biomarkers for cardiovascular disease.pdf:D\:\\Workspaces\\zotero\\storage\\E32LKR6L\\Ferri et al_2005_Lipid-modified proteins as biomarkers for cardiovascular disease.pdf:application/pdf}
