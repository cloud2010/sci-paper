	keywords = {Proteins, Phosphorylation, Amino Acid Sequence, Animals, Glycosylation, Neural Networks, Computer, Reproducibility of Results, Sensitivity and Specificity, Humans, Internet, Protein Kinases, Protein Processing, Post-Translational},
	pages = {1633--1649},
