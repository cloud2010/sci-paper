	keywords = {feature extraction, Feature extraction, Feature selection, Algorithm design and analysis, combinatorial mathematics, combinatorial optimization formulation, computational complexity, feature redundancy handling, Laplace equations, multiple output regression, optimisation, Optimization, Prediction algorithms, Redundancy, redundancy removal, regression analysis, similarity preserving, similarity preserving feature selection criteria, sparse multiple output regression formulation, sparse regularization},
	pages = {619--632},
	file = {IEEE Xplore Abstract Record:D\:\\Workspaces\\zotero\\storage\\V5CTVGJ4\\6051436.html:text/html;Zhao et al_2013_On Similarity Preserving Feature Selection.pdf:D\:\\Workspaces\\zotero\\storage\\3W5TBJI6\\Zhao et al_2013_On Similarity Preserving Feature Selection.pdf:application/pdf}
